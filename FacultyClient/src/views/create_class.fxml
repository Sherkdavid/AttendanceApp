<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="251.0" prefWidth="246.0" spacing="15.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.CreateClassController">
    <children>
      <HBox alignment="CENTER" prefHeight="43.0" prefWidth="172.0">
         <children>
            <Label styleClass="custom-label" text="New Class">
               <font>
                  <Font name="Arial" size="22.0" />
               </font>
            </Label>
         </children>
      </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="25.0" prefWidth="172.0">
            <children>
                <VBox alignment="CENTER" prefHeight="25.0" prefWidth="71.0">
                    <children>
                        <Label text="Class ID">
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font></Label>
                    </children>
                </VBox>
            <TextField fx:id="class_id" prefHeight="25.0" prefWidth="125.0" />
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="172.0">
            <children>
                <VBox alignment="CENTER" prefHeight="25.0" prefWidth="71.0">
                    <children>
                        <Label text="Lecturer">
                     <font>
                        <Font name="Arial" size="12.0" />
                     </font></Label>
                    </children>
                </VBox>
            <ComboBox fx:id="lecturer" prefHeight="25.0" prefWidth="127.0" />
            </children>
        </HBox>
      <HBox alignment="BOTTOM_CENTER" prefHeight="38.0" prefWidth="198.0" spacing="30.0">
         <children>
            <Button fx:id="cancel" mnemonicParsing="false" prefHeight="25.0" prefWidth="87.0" text="Cancel" />
            <Button fx:id="confirm" mnemonicParsing="false" prefHeight="25.0" prefWidth="87.0" text="Confirm" />
         </children>
      </HBox>
    </children>
</VBox>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox alignment="CENTER_LEFT" spacing="30.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.FacultyProfileController">
   <VBox prefHeight="241.0" prefWidth="195.0">
      <children>
            <VBox prefHeight="241.0" prefWidth="196.0" spacing="5.0">
               <children>
                  <HBox prefHeight="30.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="30.0" prefWidth="59.0">
                           <children>
                              <Label text="ID" />
                           </children>
                        </HBox>
                        <Label fx:id="id_label" text="Label" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="10.0" prefHeight="30.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="30.0" prefWidth="59.0">
                           <children>
                              <Label text="Name" />
                           </children>
                        </HBox>
                        <Label fx:id="name_label" text="Label" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="10.0" prefHeight="30.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="30.0" prefWidth="59.0">
                           <children>
                              <Label text="Dept" />
                           </children>
                        </HBox>
                        <Label fx:id="department_label" text="Label" />
                     </children>
                  </HBox>
                  <HBox layoutX="10.0" layoutY="70.0" prefHeight="30.0" prefWidth="100.0">
                     <children>
                        <HBox prefHeight="30.0" prefWidth="85.0">
                           <children>
                              <Label text="Password" />
                           </children>
                        </HBox>
                        <Button fx:id="password_button" mnemonicParsing="false" prefHeight="25.0" prefWidth="115.0" text="Change" />
                     </children>
                  </HBox>
                  <PasswordField fx:id="password_field" prefHeight="25.0" prefWidth="173.0" />
               </children>
            </VBox>
      </children></VBox>
   <VBox prefHeight="241.0" prefWidth="169.0">
      <children>
         <Label styleClass="header" text="Classes Taught" />
            <ListView fx:id="listView" maxHeight="500.0" />
      </children>
   </VBox>
</HBox>

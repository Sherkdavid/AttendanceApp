<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="376.0" prefWidth="341.0" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.RolecallController">
   <center>
      <ScrollPane fx:id="scroller" hbarPolicy="NEVER" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <content>
            <VBox fx:id="container" prefHeight="324.0" prefWidth="340.0" />
         </content>
      </ScrollPane>
   </center>
   <top>
      <HBox spacing="10.0" BorderPane.alignment="CENTER">
         <children>
            <DatePicker fx:id="date" />
            <Button mnemonicParsing="false" onAction="#submit" text="Submit" />
         </children></HBox>
   </top>
</BorderPane>

<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Menu?>
<?import javafx.scene.control.MenuBar?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="layout" xmlns="http://javafx.com/javafx/8.0.112" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.MenuController">
   <children>
      <MenuBar>
        <menus>
            <Menu fx:id="classes" mnemonicParsing="false" text="Classes" />
            <Menu fx:id="modules" mnemonicParsing="false" text="Modules" />
            <Menu fx:id="department" mnemonicParsing="false" text="Department" />
          <Menu mnemonicParsing="false" text="Account">
            <items>
                  <MenuItem mnemonicParsing="false" text="Edit" />
              <MenuItem mnemonicParsing="false" onAction="#logout" text="Log Out" />
            </items>
          </Menu>
        </menus>
      </MenuBar>
   </children>
</VBox>
